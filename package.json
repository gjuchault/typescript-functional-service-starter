{
  "name": "@gjuchault/typescript-functional-service-starter",
  "version": "0.0.0-development",
  "description": "Yet another typescript service starter template",
  "keywords": [
    "typescript",
    "service",
    "starter",
    "template"
  ],
  "homepage": "https://github.com/gjuchault/typescript-functional-service-starter",
  "bugs": "https://github.com/gjuchault/typescript-functional-service-starter/issues",
  "author": "Gabriel Juchault <gabriel.juchault@gmail.com>",
  "repository": "gjuchault/typescript-functional-service-starter",
  "main": "./build/index.js",
  "types": "./build/src/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^18.7.0 || >= 16",
    "npm": "^8.16.0 || >= 8"
  },
  "volta": {
    "node": "18.7.0",
    "npm": "8.16.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "setup": "node -r ./esbuild-hook ./scripts/setup",
    "build": "npm run clean && npm run type:dts && npm run build:main",
    "build:main": "node -r ./esbuild-hook ./scripts/build",
    "build:client": "node -r ./esbuild-hook ./scripts/client",
    "start": "node ./build",
    "start:debug": "node --inspect ./build",
    "dev": "node -r ./esbuild-hook ./scripts/dev | pino-pretty",
    "clean": "node -r ./esbuild-hook ./scripts/clean",
    "migrate:up": "node -r ./esbuild-hook ./scripts/migrate up",
    "migrate:create": "node -r ./esbuild-hook ./scripts/migrate create",
    "type:dts": "tsc --emitDeclarationOnly",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:setup": "node -r ./esbuild-hook ./scripts/testSetup",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/eslint": "^8.4.5",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.6.4",
    "@types/prompts": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "c8": "^7.11.3",
    "camelcase": "^6.3.0",
    "cspell": "^6.6.0",
    "esbuild": "^0.14.53",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-functional": "^4.2.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "get-port": "^6.1.2",
    "json-schema-to-typescript": "^11.0.2",
    "json-schema-to-zod": "^0.1.5",
    "launch-editor": "^2.5.0",
    "nyc": "^15.1.0",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "semantic-release": "^19.0.3",
    "slugify": "^1.6.5",
    "source-map-support": "^0.5.21",
    "tsutils": "^3.21.0",
    "typescript": "^4.7.4",
    "vitest": "^0.21.0"
  },
  "dependencies": {
    "@autotelic/fastify-opentelemetry": "^0.17.0",
    "@fastify/circuit-breaker": "^3.0.0",
    "@fastify/cookie": "^7.3.1",
    "@fastify/cors": "^8.1.0",
    "@fastify/etag": "^4.1.0",
    "@fastify/formbody": "^7.0.1",
    "@fastify/helmet": "^9.1.0",
    "@fastify/multipart": "^7.1.0",
    "@fastify/rate-limit": "^7.3.0",
    "@fastify/swagger": "^7.4.1",
    "@fastify/under-pressure": "^7.0.0",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/api-metrics": "^0.31.0",
    "@opentelemetry/core": "^1.5.0",
    "@opentelemetry/exporter-prometheus": "^0.31.0",
    "@opentelemetry/resources": "^1.5.0",
    "@opentelemetry/sdk-node": "^0.31.0",
    "@opentelemetry/sdk-trace-base": "^1.5.0",
    "@opentelemetry/semantic-conventions": "^1.5.0",
    "dotenv": "^16.0.1",
    "fastify": "^4.4.0",
    "fastify-plugin": "^4.1.0",
    "fp-ts": "^2.12.2",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.2.2",
    "ms": "^2.1.3",
    "pino": "^8.4.0",
    "slonik": "^30.1.2",
    "umzug": "^3.1.1",
    "zod": "^3.17.3",
    "zod-to-json-schema": "^3.17.1"
  },
  "overrides": {
    "semver-regex": "3.1.4"
  }
}
